{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/download/youtubedl",
      "methods": ["GET"],
      "dest": "/api/index.js"
    },
    {
      "src": "/download/tiktokdl",
      "methods": ["GET"],
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
