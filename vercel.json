{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/download/tiktokdl",
      "dest": "api/index.js"
    },
    {
      "src": "/download/youtubedl",
      "dest": "api/index.js"
    },
    {
      "src": "/download/instagramdl",
      "dest": "api/index.js"
    },
    {
      "src": "/download/pornhubdl",
      "dest": "api/index.js"
    },
    {
      "src": "/search/freefire",
      "dest": "api/index.js"
    },
    {
      "src": "/ephoto360",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}
